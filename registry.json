      "version": "0.1.0",
      "tags": ["rest", "api", "http", "developer"],
      "repository": "https://github.com/jongio/azd-rest"
    },
    {
      "id": "jongio.azd.script",
      "namespace": "script",
      "displayName": "Script Extension",
      "description": "Execute scripts with Azure Developer CLI context and environment variables",
      "version": "0.1.0",
      "tags": ["script", "automation", "shell", "developer"],
      "repository": "https://github.com/jongio/azd-script"
    }
  ]
